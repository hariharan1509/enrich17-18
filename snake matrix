#include<stdio.h>

int main()
{
 int i,j,n=1;
 for( i=0;i<4;i++)
 {
     if(i%2==0)
     { 
      for( j=0;j<4;j++)
      {
        printf("%d\t",n);     
            n++;
       }
       n+=4;
     }
     else
      {  
         for(j=0;j<4;j++)
         {
            n--;
             printf("%d\t",n);
             
         }
         n+=4;
      }
     printf("\n");
 }
 
}
